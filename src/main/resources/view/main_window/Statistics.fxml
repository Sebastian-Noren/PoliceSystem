<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="732.0" prefWidth="828.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pust.controller.main_window.StatisticsController">
   <children>
      <PieChart fx:id="pieChart" layoutX="242.0" prefHeight="330.0" prefWidth="586.0" />
      <Button fx:id="btnLoad" layoutX="20.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadChart" text="LOAD" />
      <BarChart fx:id="barChart" layoutY="318.0" prefHeight="400.0" prefWidth="812.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
